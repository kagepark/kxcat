groups() {
   group=$1
   if [ -n "$group" ]; then
       lsdef -t group $group | grep -v "^Object nam"
       lsdef -t osimage $group | grep -v "^Object nam"
   else
       lsdef -t group | grep -v -e "^all " -e "^N "
   fi
}
